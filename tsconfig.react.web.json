{
  "$schema": "https://json.schemastore.org/tsconfig",
  // this one came from 'react-scripts eject'
  "display": "react",
  "compilerOptions": {
    "target": "ES5",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",
    // custom/required
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.react.web.tsbuildinfo"
  }
}
